<!-- Numeric -->
<script id="IDE.Functions.FilterFunction" type="text/html">
    <h3 data-bind="text: FunctionName"></h3>
    <p>Add:</p>
    <button type="button" class="btn btn-primary"
            data-bind="click:AddStep.bind($data,'IDE.Steps.NumberStep')">
        Number
    </button>

    <button type="button" class="btn btn-primary"
            data-bind="click:AddStep.bind($data,'IDE.Steps.OperatorStep')">
        Operator
    </button>

    <div data-bind="foreach: UserSteps">
        <div class="row step">

            <div class="col-sm-1" >

                <input type="text" data-bind=" value: StepName">
            </div>
            <div data-bind="template: { name: TypeName }" class="col-sm-5">
            </div>
        </div>
    </div>
    
   
</script>
<script id="IDE.Steps.TableLoadStepDisplay" type="text/html">
</script>